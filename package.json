{
  "name": "mnn-client",
  "version": "1.0.0",
  "description": "React boilerplate for building client",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix ./src",
    "clear": "rm -rf public/assets",
    "dev": "nodemon --watch ./src --exec \"babel-node\" ./src/server/index.js",
    "build:dev": "npm run clear && NODE_ENV=development babel-node ./node_modules/.bin/webpack --progress --hide-modules --config ./webpack.config.js",
    "build:prod": "npm run clear && NODE_ENV=production babel-node ./node_modules/.bin/webpack --progress --hide-modules --config ./webpack.config.js",
    "start": "NODE_ENV=production node ./src/server/index.js"
  },
  "author": "Money Nguyen <moneynguyen0o0@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "loadable-components": "^2.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.5.0",
    "serve-favicon": "^2.5.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "eslint": "^5.4.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "nodemon": "^1.18.3",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
